{
  "name": "rk",
  "version": "0.0.0",
  "description": "Command line tools",
  "author": "Reid Knight @kinigitbyday",
  "bin": {
    "rk": "./bin/run"
  },
  "homepage": "https://github.com/kinigitbyday/rk",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "kinigitbyday/rk",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.7.0",
    "@oclif/plugin-help": "^5"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.4.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "del": "^5.1.0",
    "gifencoder": "^2.0.1",
    "jimp": "^0.9.3",
    "png-file-stream": "^1.2.1"
  },
  "oclif": {
    "bin": "rk",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "speed-up": "oclif manifest",
    "regen-readme": "oclif readme",
    "finalize": "yarn regen-readme && yarn speed-up && git add README.md oclif.manifest.json"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/kinigitbyday/rk/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
